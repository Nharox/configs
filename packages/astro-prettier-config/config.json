{
  "endOfLine": "lf",
  "importOrder": [
    "<TYPES>",
    "<TYPES>^@/",
    "<TYPES>^[./]",
    "",
    "^astro",
    "^@astrojs",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/",
    "",
    "^[./]"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
